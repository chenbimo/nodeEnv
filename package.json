{
    "name": "node-env",
    "displayName": "NodeEnv - Node.js环境管理",
    "description": "轻松管理Node.js环境和版本",
    "publisher": "chensuiyi",
    "version": "1.6.2",
    "private": true,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "./dist/extension.cjs",
    "engines": {
        "vscode": "^1.74.0"
    },
    "pricing": "Trial",
    "categories": [
        "Other"
    ],
    "icon": "dist/logo.png",
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://chensuiyi.me",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/nodeEnv"
    },
    "activationEvents": [],
    "files": [
        "dist",
        "CHANGELOG.md",
        "readme.md",
        "LICENSE"
    ],
    "scripts": {
        "pack": "vsce package --no-yarn --no-dependencies --out release --baseImagesUrl ./",
        "ext": "rollup --config rollup.config.js",
        "web": "yite --command=build --envfile=production",
        "build": "pnpm run web & pnpm run ext"
    },
    "dependencies": {
        "@arco-design/web-vue": "^2.55.2",
        "@arco-plugins/vite-vue": "^1.4.5",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@vscode/codicons": "^0.0.36",
        "@vscode/vsce": "^2.27.0",
        "@yicode/yidash": "^3.8.3",
        "@yicode/yite-cli": "^2.6.21",
        "adm-zip": "^0.5.14",
        "fs-extra": "^11.2.0",
        "got": "^14.4.0",
        "ini": "^4.1.3",
        "pinia": "^2.1.7",
        "rollup": "^4.18.0",
        "sass": "^1.77.4",
        "store2": "^2.14.3",
        "tar-fs": "^3.0.6",
        "vue": "^3.4.27",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.3.2"
    },
    "contributes": {
        "commands": [
            {
                "command": "fnMap.forceRefreshVersions",
                "title": "刷新版本列表",
                "icon": "$(refresh)"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "nodeEnvExplorer",
                    "title": "NodeEnv",
                    "icon": "dist/logo.svg"
                }
            ]
        },
        "views": {
            "nodeEnvExplorer": [
                {
                    "id": "nodeEnvWebView",
                    "type": "webview",
                    "name": "Nodejs环境管理"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "fnMap.forceRefreshVersions",
                    "when": "view == nodeEnvWebView",
                    "group": "navigation@1"
                }
            ]
        }
    }
}
